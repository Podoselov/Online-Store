{
  "name": "sneakers-store",
  "version": "0.1.0",
  "private": true,
  "proxy": "http://localhost:5000",
  "dependencies": {
    "@emotion/react": "^11.8.1",
    "@emotion/styled": "^11.8.1",
    "@mui/icons-material": "^5.4.4",
    "@mui/material": "^5.4.4",
    "@mui/styles": "^5.4.4",
    "@mui/system": "^5.4.4",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.3",
    "@testing-library/user-event": "^13.5.0",
    "axios": "^0.26.0",
    "call-bind": "^1.0.2",
    "core-js": "^3.21.1",
    "formik": "^2.2.9",
    "json-server": "^0.17.0",
    "mongoose": "^6.2.9",
    "npm-run-all": "^4.1.1",
    "npm-watch": "^0.11.0",
    "prop-types": "^15.8.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-elastic-carousel": "^0.11.5",
    "react-redux": "^7.2.6",
    "react-router-dom": "^6.2.2",
    "react-scripts": "^2.1.3",
    "react-swipeable-views": "^0.14.0",
    "redux": "^4.1.2",
    "redux-persist": "^6.0.0",
    "redux-thunk": "^2.4.1",
    "sass": "^1.49.9",
    "stylelint": "^14.5.3",
    "stylelint-config-rational-order": "^0.0.4",
    "stylelint-order": "^5.0.0",
    "swiper": "^8.0.7",
    "web-vitals": "^2.1.4",
    "yup": "^0.32.11"
  },
  "watch": {
    "stylelint-dev": {
      "patterns": [
        "/src/**/*.scss"
      ],
      "extensions": "scss"
    },
    "eslint": {
      "patterns": [
        "/src/**/*.js"
      ],
      "extensions": "js"
    }
  },
  "scripts": {
    "start": "npm-run-all -p watch-sass-js start-js",
    "start-js": "react-scripts start",
    "build": "npm run eslint && npm run stylelint-build && react-scripts build",
    "eslint": "eslint 'src/**/*.js'",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "stylelint-dev": "stylelint src/**/*.scss --cache --cache-location \"./.stylelintcache\" ; exit 0",
    "stylelint-build": "stylelint 'src/**/*.scss'",
    "watch-sass-js": "npm-watch",
    "lint": "eslint --debug src/",
    "lint:write": "eslint --debug src/ --fix",
    "server": "json-server --watch db.json --port 5000"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.17.0",
    "@babel/plugin-transform-spread": "^7.16.7",
    "@babel/preset-react": "^7.16.7",
    "eslint": "5.12.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.4.0",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "prettier": "^2.5.1",
    "prettier-eslint": "^8.2.2",
    "standard": "^16.0.4"
  }
}
